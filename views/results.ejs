  <h1>Results</h1>
<div id="results">
  <% showlist.results.forEach(function(show){ %>
  	 <div class="row">
        <div class="col s12 m6 l6">
          <div class="card-panel teal lighten-4">
            <span class="white-text">
            	<a href="/results/show/<%=show.id%>"><%= show.title %></a>
            </span>
          </div>
        </div>
    </div>
  <% }); %>
</div>
<div id="chair" class="responsive-img">
  <img src="/tv-chair.jpg">
</div>
