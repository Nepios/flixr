  <h1>Results</h1>

  <% showlist.results.forEach(function(show){ %>
  	 <div class="row">
        <div class="col s12 m5">
          <div class="card-panel #b0bec5 blue-grey lighten-3">
            <span class="white-text">
            	<a href="/results/show/<%=show.id%>"><%= show.title %></a>
            </span>
          </div>
        </div>
    </div>
  <% }); %>

