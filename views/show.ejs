<div class="center-align">
	<div class="row">
		<div class="col s12 m7">
		  <div class="card">
		    <div class="card-image">
		      <img src="<%= movieData.artwork_448x252 %>">

		    </div>
		    <div class="card-content center-align">
		      <p><%= movieData.overview %>
		      <% platforms.results.web.episodes.all_sources.forEach(function(platform){ %>
		      	<h6><%= platform.display_name %></h6>
		      <% }); %>
		      </p>

		    </div>
		    <div class="card-action">
		      <a href="#">Add to Favorites</a>
		    </div>
		      <div class="card-action">
		      <a href="#">Add to Currently Watching</a>
		    </div>
		  </div>
		</div>
	</div>
</div>