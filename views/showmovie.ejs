<h1>show movie</h1>
<div class="center-align">
	<div class="row">
		<div class="col s12 m7">
		  <div class="card">
		    <div class="card-image">
		      <img src="<%= movieData.poster_240x342 %>">

		    </div>
		    <div class="card-content left-align">
		      <p><%= movieData.overview %>
		      <% platforms.results.web.episodes.all_sources.forEach(function(platform){ %>
		      		<% var list = []; %>
		      		<% list.push(platform.display_name); %>
		      		<h6><%= list %></h6>
		      <% }); %>

		      </p>

		    </div>
		    <div class="card-action">
		      <a href="/favorites/">Add to Favorites</a>
		    </div>
		      <div class="card-action">
		      <a href="#">Add to Currently Watching</a>
		    </div>
		  </div>
		</div>
	</div>
</div>