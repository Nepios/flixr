<h1>Users</h1>

<% user.forEach(function(individual){ %>
   <div class="row">
      <div class="col s12 m5">
        <div class="card-panel teal lighten-4">
          <span class="white-text">
              <p>
                <a href="/user/<%= individual.id %>"><%= individual.name %></a>
 
              </p>
          </span>
        </div>
      </div>
    </div>
<% }) %>