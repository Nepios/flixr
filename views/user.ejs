<h1>Users</h1>

<% user.forEach(function(individual){ %>
   <div class="row">
      <div class="col s12 m5">
        <div class="card-panel #b0bec5 blue-grey lighten-3">
          <span class="white-text">
              <p><a href="/user/<%= individual.id %>"><%= individual.name %></a></p>
          </span>
        </div>
      </div>
    </div>
<% }) %>